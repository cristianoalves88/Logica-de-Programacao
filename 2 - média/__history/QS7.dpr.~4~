program QS7;

{$APPTYPE CONSOLE}

uses
  uTroco in 'uTroco.pas',
  uIMaquina in 'uIMaquina.pas',
  uMaquina in 'uMaquina.pas',
  uFuncoes in 'uFuncoes.pas';

var
  sTroco:TMaquina;
  vTroco:string;

{$R *.RES}


begin
	writeLn ( 'Teste QS7' ) ;
  Writeln (' ');
	writeLn ( 'Digite um Valor.' ) ;
  readLn  (vTroco);
  sTroco.MontarTroco(uFuncoes.fConverte(vTroco));

end.

